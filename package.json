{
    "name": "bhailang",
    "displayName": "BhaiLang",
    "publisher": "AronyaBaksy",
    "description": "Language support for BhaiLang",
    "icon": "./img/bhai_logo.jpeg",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.65.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/abaksy/bhailang-vscode"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "bhailang",
                "aliases": [
                    "BhaiLang",
                    "bhailang"
                ],
                "extensions": [
                    ".bhai"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "bhailang",
                "scopeName": "source.bhai",
                "path": "./syntaxes/bhailang.tmLanguage.json"
            }
        ]
    }
}
